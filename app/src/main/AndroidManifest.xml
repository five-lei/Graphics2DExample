<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yilei.graphics2dexample">

    <application
        android:name=".application.GApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.DynamicActivity" />
        <activity android:name=".view.activity.InvalidateActivity" />
        <activity android:name=".view.activity.CoordinateActivity" />
        <activity android:name=".view.activity.ClippingActivity" />
        <activity android:name=".view.activity.WatchActivity"></activity>
    </application>

</manifest>